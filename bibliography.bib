	Doi = {10.1016/j.icarus.2008.03.011},
	Journal = {Icarus},
	Month = {Aug},
	Pages = {518},
	Pmid = {2008Icar..196..518C},
	Rating = {0},
	Read = {Yes},
	Title = {Lunar-forming collisions with pre-impact rotation},
